{
  "name": "F# (.NET 7.0)",
  "dockerFile": "Dockerfile",
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.shell.linux": "/bin/bash",
        "FSharp.useSdkScripts": true,
        "FSharp.workspacePath": "FsAutoComplete.sln"
      },
      "extensions": [
        "Ionide.Ionide-fsharp",
        "ms-vscode.csharp"
      ]
    }
  },
  "onCreateCommand": "dotnet restore"
}